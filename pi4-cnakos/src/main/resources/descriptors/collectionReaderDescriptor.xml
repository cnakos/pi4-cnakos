<?xml version="1.0" encoding="UTF-8"?>
<collectionReaderDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <implementationName>QuestionPassageReader</implementationName>
  <processingResourceMetaData>
    <name>collectionReaderDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>InputDir</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>InputDir</name>
        <value>
          <string>src/main/resources/inputData</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="descriptors.typeSystem"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type>type.Question</type>
          <type>type.Passage</type>
          <type>type.InputDocument</type>
          <feature>type.Question:passages</feature>
          <feature>type.Question:sentence</feature>
          <feature>type.Question:id</feature>
          <feature>type.Question:score</feature>
          <feature>type.Question:componentId</feature>
          <feature>type.Question:end</feature>
          <feature>type.Question:begin</feature>
          <feature>type.Passage:originalText</feature>
          <feature>type.Passage:question</feature>
          <feature>type.Passage:sourceDocId</feature>
          <feature>type.Passage:label</feature>
          <feature>type.Passage:text</feature>
          <feature>type.Passage:componentId</feature>
          <feature>type.Passage:end</feature>
          <feature>type.Passage:begin</feature>
          <feature>type.InputDocument:questions</feature>
          <feature>type.InputDocument:passages</feature>
          <feature>type.InputDocument:score</feature>
          <feature>type.InputDocument:componentId</feature>
          <feature>type.InputDocument:end</feature>
          <feature>type.InputDocument:begin</feature>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>true</outputsNewCASes>
    </operationalProperties>
  </processingResourceMetaData>
  <resourceManagerConfiguration/>
</collectionReaderDescription>
